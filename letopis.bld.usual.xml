<?xml version="1.0" encoding="UTF-8" ?>
<bld>
  <tex_exe>pdflatex</tex_exe>
  <sii>
    <insert titletoc="1" hyperlinks="1" />
    <scts>
      <sct name="preamble.packages">
        <pack_list>
           ifxetex ifpdf
           xparse
           titletoc
           mathtext
           iflang
           tocloft
           nameref
           color
           xcolor
           colortbl
           imakeidx
           hyperref
           bookmark
           geometry
           etoolbox
           adjustbox
           ifthen
           longtable
           graphicx
           projs
           multicol
           pgffor
           filecontents
           tikz
           datetime2
        </pack_list>
        <pack_opts>
          <datetime2>
           useregional
          </datetime2>
          <tocloft>
           titles
          </tocloft>
          <imakeidx>
           xindy
          </imakeidx>
          <hyperref>
           colorlinks=true
           linktoc=all
           linkcolor=blue
           letterpaper
           unicode
           linktocpage
           bookmarksdepth=subparagraph
           bookmarksnumbered=true
          </hyperref>
          <geometry>
           hmargin={1cm,1cm},vmargin={2cm,2cm},centering
          </geometry>
          <adjustbox>
           export
          </adjustbox>
        </pack_opts>
      </sct>
      <sct name="preamble.packages.xelatex">
        <text>
        </text>
      </sct>
    </scts>
    <generate>
      <on>
        preamble.packages
        preamble.packages.xelatex
        preamble.packages.pdflatex
      </on>
    </generate>
  </sii>
  <opts_maker>
    <skip get_opt="1" />
    <load_dat ii_include="1" ii_exclude="1" />
    <join_lines>
      <include_below>
        section
      </include_below>
    </join_lines>
    <sections>
      <include>
      </include>
      <include_with_children>
        preamble
      </include_with_children>
      <ins_order>
         hyperlinks
         titletoc
      </ins_order>
      <generate>
      </generate>
      <append>
        <each>
        </each>
        <only>
          <defs>
          </defs>
        </only>
      </append>
    </sections>
  </opts_maker>
</bld>
